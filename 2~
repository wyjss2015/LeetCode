struct ListNode* addTwoNumbers(struct ListNode* l1, struct ListNode* l2) {
    int x = 0;
    struct ListNode* l3,p,q,r;
    l3 = (struct ListNode*)malloc(sizeof(struct ListNode));
    p = l1;
    q = l2;
    r = l3;
    while(p!=NULL && q!=NULL){
        if(p->val+q->val>=10){
            x = 1;
            l3->val = p->val+q->val-10;
        }else{
            x = 0;
            l3->val = p->val+q->val;
        }
        
    }
}
